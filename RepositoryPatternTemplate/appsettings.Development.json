{

  // DB Conection string
  "ConnectionStrings": {
    "DbConnection": "server=[$SERVER]; port=[$PORT]; database=[$DATABASE]; uid=[$UID]; password=[$PASSWORD]"
  },

  "Cors": {
    "Domains": [ "http://localhost:4200" ]
  },

  "AllowedHosts": "*",

  // MySQL Version

  "MySqlVersion": {
    "Major": "[$MAJOR]",
    "Minor": "[$MINOR]",
    "Build": "[$BUILD]"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
